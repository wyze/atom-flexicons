{
  "name": "flexicons",
  "version": "0.0.0",
  "description": "A flexible full-color icon package for use in Atom.",
  "license": "MIT",
  "repository": "wyze/atom-flexicons",
  "author": {
    "name": "Neil Kistner",
    "email": "neil.kistner@gmail.com",
    "url": "neilkistner.com"
  },
  "main": "lib/index",
  "scripts": {
    "codecov": "nyc report -r text-lcov | codecov",
    "lint": "eslint bin lib test",
    "test": "nyc ava",
    "test:watch": "npm test -- --watch",
    "build": "node bin/build",
    "prebuild": "npm run test",
    "pretest": "npm run lint",
    "preversion": "npm run build"
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "babel-register"
    ]
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-1"
    ]
  },
  "eslintConfig": {
    "extends": "wyze/base",
    "globals": {
      "atom": true
    }
  },
  "files": [
    "lib",
    "styles"
  ],
  "keywords": [],
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "styles": [
    "flexicons.less"
  ],
  "devDependencies": {
    "ava": "^0.13.0",
    "babel-eslint": "^6.0.0-beta.3",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-register": "^6.7.2",
    "codecov.io": "^0.1.6",
    "eslint": "~2.2.0",
    "eslint-config-wyze": "^1.2.1",
    "nyc": "^6.0.0"
  },
  "dependencies": {
    "lodash.filter": "^4.2.1"
  }
}
